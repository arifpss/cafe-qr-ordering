{
  "name": "cafe-qr-ordering-webapp",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "npm run build && wrangler pages dev dist",
    "build": "vite build",
    "preview": "wrangler pages dev dist",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "cf:init": "node scripts/cf-init.mjs",
    "cf:migrate:local": "node scripts/cf-migrate.mjs --local",
    "cf:migrate:remote": "node scripts/cf-migrate.mjs --remote",
    "cf:seed": "node scripts/cf-seed.mjs",
    "cf:secrets": "node scripts/cf-secrets.mjs",
    "deploy": "node scripts/deploy.mjs"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "hono": "^4.5.5",
    "qrcode-generator": "^2.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240913.0",
    "@eslint/js": "^9.11.1",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vitest": "^2.1.1",
    "wrangler": "^3.79.0"
  }
}
